<PropertyList>
 <path>pres4_2.ac</path>

   <!--Lights-->
                                   
 <animation>   
   <object-name>group</object-name>
   <object-name>n1</object-name>
   <object-name>n2</object-name>   
</animation>

  <animation>
    <type>material</type>
    <object-name>group</object-name>
   <object-name>n1</object-name>
   <object-name>n2</object-name> 
    <emission>
      <factor-prop>controls/switches/gauge-light</factor-prop>
      <red>   0.35 </red>
      <green> 0.35 </green>
      <blue>  0.35 </blue>
    </emission>
  </animation>

 
  <animation>
  <type>rotate</type>
  <object-name>n2</object-name>
 <property>instrumentation/fuel_press/A0/press1</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>0</dep></entry> 

   <entry><ind> 4</ind><dep> 120</dep></entry>
  
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.019</y-m>
   <z-m>0.019</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
   <animation>
  <type>rotate</type>
  <object-name>n1</object-name>
 <property>instrumentation/fuel_press/A0/press2</property>
  <interpolation>
   <entry><ind>  0.0</ind><dep>0</dep></entry> 

   <entry><ind> 4</ind><dep> 120</dep></entry>
  
  </interpolation>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.019</y-m>
   <z-m>-0.019</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
 
 </PropertyList>

